{
    "name": "aleahy/saasu-connect",
    "description": "Connect to and use the Saasu API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexander Leahy",
            "email": "alex@languagetub.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
